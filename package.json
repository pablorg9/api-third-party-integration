{
    "name": "api-third-party-integration",
    "version": "0.0.0",
    "main": "dist/Server.js",
    "description": "Api for third party integrations",
    "license": "UNLICENSED",
    "private": true,
    "author": {
        "email": "pablofrg98@gmail.com",
        "name": "Juan Pablo Restrepo"
    },
    "scripts": {
        "build": "rimraf dist && tsc -p tsconfig.build.json",
        "lint": "eslint . --ext .ts --fix",
        "test": "jest --color --verbose --no-cache",
        "format": "prettier --write \"./src/**/*.ts\"",
        "format-check": "prettier --list-different \"./src/**/*.ts\"",
        "dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --require dotenv/config dist/Server.js\"",
        "start": "node --require dotenv/config dist/Server.js",
        "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/Server.js\"",
        "app-deploy": "gcloud app deploy",
        "pre-commit": "yarn lint && yarn test"
    },
    "dependencies": {
        "axios": "^0.21.4",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^9.0.2",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-jwt": "^6.0.0",
        "helmet": "^4.6.0",
        "inversify": "^5.1.1",
        "method-override": "^3.0.0",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "reflect-metadata": "^0.1.13",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.10",
        "@types/errorhandler": "^1.5.0",
        "@types/express": "^4.17.11",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^27.0.1",
        "@types/method-override": "^0.0.31",
        "@types/morgan": "^1.9.2",
        "@types/node": "^15.3.0",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^4.2.5",
        "jest": "^27.2.0",
        "prettier": "^2.3.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.5",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.2.4"
    },
    "_moduleAliases": {
        "@dao": "dist/dao",
        "@services": "dist/services",
        "@routers": "dist/routers",
        "@setup": "dist/setup"
    },
    "engines": {
        "node": "14.x.x"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn pre-commit",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
